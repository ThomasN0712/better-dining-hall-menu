{
  "version": 2,
  "builds": [
    {
      "src": "app/backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "rootDirectory": "app"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
